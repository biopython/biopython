<!-- ================================================================= -->

<!ELEMENT       Id              (#PCDATA)>		<!-- \d+ -->
<!ELEMENT       Item        	(#PCDATA|Item)* >	<!-- .+ -->
<!ATTLIST Item
    Name CDATA                         #REQUIRED
    Type (Integer|Date|String|List|Unknown) #REQUIRED
>

<!ELEMENT        ERROR          (#PCDATA)>		<!-- .+ -->

<!ELEMENT        DocSum         (Id, Item+)>

<!ELEMENT        eSummaryResult    (DocSum|ERROR)+>
